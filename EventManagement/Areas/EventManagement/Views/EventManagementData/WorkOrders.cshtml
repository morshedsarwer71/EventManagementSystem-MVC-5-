@model EventManagement.Areas.EventManagement.ViewModels.WorkOrderViewModels

@{
    Layout = null;
}
<section class="content-header">
    <h1>
        Orders
        <small>preview of work orders</small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i>Home</a></li>
        <li><a href="#">Work Order</a></li>
        <li class="active">Work Orders</li>
    </ol>
</section>

<!-- Main content -->
<section class="content">
    <div class="row">
        <div class="col-md-12">
            <div class="box">
                <!-- /.box-header -->
                <div class="box-body">
                    <table class="table table-bordered">
                        <thead>
                            <tr>
                                <th style="width: 10px">#</th>
                                <th>Code</th>
                                <th>Client</th>
                                <th>Men Power</th>
                                <th>Per Hour Rate</th>
                                <th>Start Date</th>
                                <th>End Date</th>
                                <th>Total Days</th>
                                <th>Status</th>
                                <th>Notes</th>
                                <th>Details</th>
                                <th>Edit</th>
                            </tr>
                        </thead>
                        <tbody id="tBody">
                            @foreach (var item in Model.WorkOrderParents)
                            {
                                <tr>
                                    <td>@item.SerialNumber</td>
                                    <td>@item.WorkOrderCode</td>
                                    <td>@item.ClientName</td>
                                    <td>@item.NumberOfManpower</td>
                                    <td>@item.PerHourRate</td>
                                    <td>@item.StartingDate</td>
                                    <td>@item.EndingDate</td>
                                    <td>@item.TotalDays</td>
                                    <td>@item.Status</td>
                                    <td>@item.Notes</td>
                                    <td><a href="#"><i class="fa fa-eye" style="font-size:20px;"></i></a></td>
                                    <td><a href="@Url.Action("EditWorkOrder","EventManagementData",new {id=item.OrderId })"><i class="fa fa-edit" style="font-size:20px;"></i></a></td>

                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
                <!-- /.box-body -->
                <div class="box-footer clearfix">

                    <ul class="pagination pagination-sm no-margin pull-right">
                        <li><a href="#">&laquo;</a></li>
                        <li><a href="#">1</a></li>
                        <li><a href="#">2</a></li>
                        <li><a href="#">3</a></li>
                        <li><a href="#">&raquo;</a></li>
                    </ul>
                </div>
            </div>
            <!-- /.box -->
        </div>
        <!-- /.col -->

    </div>
</section>