@model IEnumerable<EventManagement.Areas.EventManagement.Models.ExpenditureHead>

@{
    Layout = null;
}
<section class="content-header">
    <h1>
        Expenditure Heads
        <small>preview of expenditure heads</small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i>Home</a></li>
        <li><a href="#">Expenditure</a></li>
        <li class="active">Expenditures</li>
    </ol>
</section>

<!-- Main content -->
<section class="content">
    <div class="row">
        <div class="col-md-12">
            <div class="box">
                @{ var sl = 0;}
                <!-- /.box-header -->
                <div class="box-body">
                    <table class="table table-bordered">
                        <thead>
                            <tr>
                                <th style="width: 10px">#</th>
                                <th>Expenditure EN</th>
                                <th>Expenditure AR</th>
                                <th>Edit</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                sl++;
                                <tr>
                                    <td>@sl</td>
                                    <td>@item.ExpenditureHeadEN</td>
                                    <td>@item.ExpenditureHeadAR</td>
                                    <td><a href="#"><i class="fa fa-edit"></i></a></td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
                <!-- /.box-body -->
                <div class="box-footer clearfix">

                    <ul class="pagination pagination-sm no-margin pull-right">
                        <li><a href="#">&laquo;</a></li>
                        <li><a href="#">1</a></li>
                        <li><a href="#">2</a></li>
                        <li><a href="#">3</a></li>
                        <li><a href="#">&raquo;</a></li>
                    </ul>
                </div>
            </div>
            <!-- /.box -->
        </div>
        <!-- /.col -->

    </div>
</section>