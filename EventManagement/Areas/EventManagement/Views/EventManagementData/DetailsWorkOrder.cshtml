@model EventManagement.Areas.EventManagement.ViewModels.WorkOrderViewModels
<section class="content-header">
    <h1>
        Work Order Details
        <small>details work order information</small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="@Url.Action("#")"><i class="fa fa-dashboard"></i> Home</a></li>
    </ol>
</section>

<!-- Main content -->
<section class="content">
    <div class="row">
        <div class="box">
            <!-- /.box-header -->
            <div class="box-body">
                <div class="col-md-8 col-sm-8">
                        <div class="col-md-4">
                            <div class="form-group">
                                <label class="col-form-label">Order Code</label>
                                <div class="input-group">
                                    <div class="input-group-addon">
                                        <i class="fa fa-calendar"></i>
                                    </div>
                                    <input type="text" value="@Model.WorkOrderParent.OrderCode" readonly="readonly" class="form-control" name="OrderCode">
                                </div>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label class="col-form-label">Order VAT Code</label>
                                <div class="input-group">
                                    <div class="input-group-addon">
                                        <i class="fa fa-calendar"></i>
                                    </div>
                                    <select class="form-control" name="VATCode" readonly="readonly">
                                        @foreach (var item in Model.VAT)
                                        {
                                            if (item.SettingId == Model.WorkOrderParent.VATCode)
                                            {
                                                <option selected="selected" value="@item.SettingId">@item.VatName</option>
                                            }
                                            else
                                            {
                                                <option value="@item.SettingId">@item.VatName</option>
                                            }

                                        }
                                    </select>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label class="col-form-label">Order Client </label>
                                <div class="input-group">
                                    <div class="input-group-addon">
                                        <i class="fa fa-calendar"></i>
                                    </div>
                                    <select class="form-control" name="ClientId" readonly="readonly">
                                        @foreach (var item in Model.EventClients)
                                        {
                                            if (item.ClientId == Model.WorkOrderParent.ClientId)
                                            {
                                                <option selected="selected" value="@item.ClientId">@item.ClientNameEN</option>
                                            }
                                            else
                                            {
                                                <option value="@item.ClientId">@item.ClientNameEN</option>

                                            }
                                        }
                                    </select>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label class="col-form-label">No. Of Manpower </label>
                                <div class="input-group">
                                    <div class="input-group-addon">
                                        <i class="fa fa-calendar"></i>
                                    </div>
                                    <input type="text" readonly="readonly" value="@Model.WorkOrderParent.NoOfManpower" class="form-control" name="NoOfManpower">
                                </div>
                            </div>
                        </div>

                        <div class="col-md-4">
                            <div class="form-group">
                                <label class="col-form-label">No. Of Setup </label>
                                <div class="input-group">
                                    <div class="input-group-addon">
                                        <i class="fa fa-calendar"></i>
                                    </div>
                                    <input type="text" readonly="readonly" value="@Model.WorkOrderParent.NoOfSetup" class="form-control" name="NoOfSetup">
                                </div>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label class="col-form-label">No. Of Service</label>
                                <div class="input-group">
                                    <div class="input-group-addon">
                                        <i class="fa fa-calendar"></i>
                                    </div>
                                    <input type="text" readonly="readonly" value="@Model.WorkOrderParent.NoOfService" class="form-control" name="NoOfService">
                                </div>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label class="col-form-label">No. Of Miscell </label>
                                <div class="input-group">
                                    <div class="input-group-addon">
                                        <i class="fa fa-calendar"></i>
                                    </div>
                                    <input type="text" readonly="readonly" value="@Model.WorkOrderParent.NoOfPax" class="form-control" name="NoOfPax">
                                </div>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label class="col-form-label">Payment Status</label>
                                <div class="input-group">
                                    <div class="input-group-addon">
                                        <i class="fa fa-calendar"></i>
                                    </div>
                                    <select class="form-control" name="PaymentStatus" readonly="readonly">
                                        @foreach (var item in Model.PaymentStatus)
                                        {
                                            if (item.Id == Model.WorkOrderParent.VATCode)
                                            {
                                                <option selected="selected" value="@item.Id">@item.Name</option>
                                            }
                                            else
                                            {
                                                <option value="@item.Id">@item.Name</option>
                                            }
                                        }
                                    </select>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label class="col-form-label">Per Hour Rate</label>
                                <div class="input-group">
                                    <div class="input-group-addon">
                                        <i class="fa fa-calendar"></i>
                                    </div>
                                    <input type="text" readonly="readonly" value="@Model.WorkOrderParent.PerHourRate" class="form-control" name="PerHourRate">
                                </div>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label class="col-form-label">Starting Date</label>
                                <div class="input-group">
                                    <div class="input-group-addon">
                                        <i class="fa fa-calendar"></i>
                                    </div>
                                    <input type="text" readonly="readonly" value="@Model.WorkOrderParent.StartingDate.ToShortDateString()" class="form-control" name="StartingDate" id="startingDate">
                                </div>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label class="col-form-label">Ending Date</label>
                                <div class="input-group">
                                    <div class="input-group-addon">
                                        <i class="fa fa-calendar"></i>
                                    </div>
                                    <input type="text" readonly="readonly" value="@Model.WorkOrderParent.EndingDate.ToShortDateString()" class="form-control" name="EndingDate" id="endingDate">
                                </div>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label class="col-form-label">Status<span style="color:red;"> *</span></label>
                                <div class="input-group">
                                    <div class="input-group-addon">
                                        <i class="fa fa-calendar"></i>
                                    </div>
                                    <select class="form-control" name="Status" readonly="readonly">
                                        @foreach (var item in Model.WorkOrderStatus)
                                        {
                                            if (item.OrderStatusId == Model.WorkOrderParent.Status)
                                            {
                                                <option selected="selected" value="@item.OrderStatusId">@item.OrderStates</option>
                                            }
                                            else
                                            {
                                                <option value="@item.OrderStatusId">@item.OrderStates</option>
                                            }
                                        }
                                    </select>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label class="col-form-label">Notes <span style="color:red;"> *</span></label>
                                <div class="input-group">
                                    <div class="input-group-addon">
                                        <i class="fa fa-calendar"></i>
                                    </div>
                                    <textarea readonly="readonly" class="form-control" name="Notes">@Model.WorkOrderParent.Notes</textarea>
                                </div>
                            </div>

                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label class="col-form-label">Description <span style="color:red;"> *</span></label>
                                <div class="input-group">
                                    <div class="input-group-addon">
                                        <i class="fa fa-calendar"></i>
                                    </div>
                                    <textarea class="form-control" readonly="readonly" name="Description">@Model.WorkOrderParent.Description</textarea>
                                </div>
                            </div>
                        </div>

                        <div class="col-md-12">
                            <div class="form-group">
                                <a class="btn btn-primary" href="@Url.Action("WorkOrder","EventManagementData",new {Area="EventManagement" })">BACK</a>
                            </div>
                        </div>                    
                </div>
            </div>
        </div>

    </div>
</section>